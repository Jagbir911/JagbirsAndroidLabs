<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <TextView
        android:id="@+id/messageTextt"
        android:layout_width="238dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:text="Message Goes here"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/timeTextt"
        android:layout_width="235dp"
        android:layout_height="35dp"
        android:layout_marginTop="32dp"
        android:text="Time Goes Here"
        app:layout_constraintStart_toStartOf="@+id/messageTextt"
        app:layout_constraintTop_toBottomOf="@+id/messageTextt" />

    <TextView
        android:id="@+id/sendReceiveTextt"
        android:layout_width="240dp"
        android:layout_height="35dp"
        android:layout_marginTop="32dp"
        android:text="Send Receive Goes Here"
        app:layout_constraintStart_toStartOf="@+id/timeTextt"
        app:layout_constraintTop_toBottomOf="@+id/timeTextt" />

    <TextView
        android:id="@+id/databaseText"
        android:layout_width="259dp"
        android:layout_height="34dp"
        android:layout_marginTop="32dp"
        android:text="Database id Goes here"
        app:layout_constraintStart_toStartOf="@+id/sendReceiveTextt"
        app:layout_constraintTop_toBottomOf="@+id/sendReceiveTextt" />
</androidx.constraintlayout.widget.ConstraintLayout>